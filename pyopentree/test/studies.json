{
    "test_find_studies": {
        "test_function": "studies_find_studies",
        "test_input": {"study_property":"'ot:studyId'",
                       "value":"'pg_719'",
                       "verbose":"True"},
        "tests": {
            "contains": [
                ["matched_studies","Output doesn't contain matched_studies"]
                ],
            "equals": [
                [["'matched_studies'","[]"],"Fails that results are actually empty"]
                ]
        }
    },
